    div(data-bind="if: $data.name() === 'Font Awesome'")
        blockquote
            small Font Awesome gives you scalable vector icons that can instantly be customized — size, color, drop shadow, and anything that can be done with the power of CSS.
        br
        p
            //- Links
            ul.list-inline
                li.text-center
                    a(href="http://fontawesome.io/" title="Font Awesome Website")
                        i.fa.fa-globe.fa-2x
                        br
                        small Website
                li.text-center
                    a(href="https://github.com/FortAwesome/Font-Awesome" title="Github Repository")
                        i.fa.fa-github.fa-2x
                        br
                        small Source Code
                li.text-center
                    a(href="https://twitter.com/fontawesome" title="Follow on Twitter")
                        i.fa.fa-twitter.fa-2x
                        br
                        small Twitter

